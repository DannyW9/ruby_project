<h1>Current Members List</h1>

 <% @members.each do |member| %>
 <div class="member_div">
   <h3> <%= member.full_name() %></h3>
   <p> Membership Type: <%= member.membership_type %> </p>

   <form action="/members/<%= member.id %>" method="get">
     <input class="details_button" type="submit" value="View More Details">
   </form>


 </div>
  <hr>
 <% end %>
